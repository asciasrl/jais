<?xml version="1.0" encoding="ISO-8859-1" ?>
<jais:configuration version="1.3.1">
  <AUI>
    <startPage>Hbox</startPage>
    <title>ASCIA HomeInTheBox [AUI 0.1]</title>
    <skin>skins/20090330/</skin>
    <!-- definizione di tutti i tipi di controlli -->
    <controls>
      <page>
        <default>images/page_back.png</default>
      </page>    
      <light>
        <default>images/light_off.png</default>
        <on>images/light_on.png</on>
        <off>images/light_off.png</off>  
      </light>
      <pushbutton>
        <default>images/pushbutton_green-off.png</default>
        <on>images/pushbutton_green-on.png</on>
        <off>images/pushbutton_green-off.png</off>  
      </pushbutton>
      <group>
      	<controlclass>Scene</controlclass>
        <default>images/pushbutton_green-off.png</default>
        <on>images/pushbutton_green-on.png</on>
        <off>images/pushbutton_green-off.png</off>  
      </group>
      <dimmer>
        <default>images/light_off.png</default>
        <on>images/light_on.png</on>
        <off>images/light_off.png</off>
        <dimming>images/light_dimming.png</dimming>
        <up>images/light_up.png</up>
        <down>images/light_down.png</down>  
        <timer>500</timer>
      </dimmer>
      <blind>
        <default>images/blind_stopped.png</default>
        <stopped>images/blind_stopped.png</stopped>
        <open>images/blind_open.png</open>
        <opening>images/blind_opening.png</opening>
        <opened>images/blind_opened.png</opened>
        <close>images/blind_close.png</close>
        <closing>images/blind_closing.png</closing>
        <closed>images/blind_closed.png</closed>
      </blind>
      <digitalinput>
        <default>images/light_off.png</default>
        <true>images/light_on.png</true>
        <false>images/light_off.png</false>
      </digitalinput>
      <power>
        <default>images/power_off.png</default>
        <on>images/power_on.png</on>
        <off>images/power_off.png</off>
      </power>
      <scene>
        <default>images/scene_off.png</default>
        <off>images/scene_off.png</off>
        <on>images/scene_on.png</on>
      </scene>
      <thermo>
        <default>images/thermo_display.png</default>
        <crono>images/thermo_crono.png</crono>
        <down>images/thermo_down.png</down>
        <fan0>images/thermo_fan0.png</fan0>
        <fan1>images/thermo_fan1.png</fan1>
        <fan2>images/thermo_fan2.png</fan2>
        <fan3>images/thermo_fan3.png</fan3>
        <manual_off>images/thermo_manual_off.png</manual_off>
        <manual_on>images/thermo_manual_on.png</manual_on>
        <switch_on>images/thermo_switch_on.png</switch_on>
        <T1_off>images/thermo_T1_off.png</T1_off>
        <T1_on>images/thermo_T1_on.png</T1_on>
        <T2_off>images/thermo_T2_off.png</T2_off>
        <T2_on>images/thermo_T2_on.png</T2_on>
        <T3_off>images/thermo_T3_off.png</T3_off>
        <T3_on>images/thermo_T3_on.png</T3_on>
        <up>images/thermo_up.png</up>
        <dn>images/thermo_dn.png</dn>
      </thermo>   
      <webcam>
        <default>images/webcam-pause.png</default>
        <pause>images/webcam-pause.png</pause>
        <play>images/webcam-play.png</play>
      </webcam>   
    </controls>
    <!-- definizione di tutti i layers utilizzabili nelle pagine -->
    <layers>
      <layer id="scene">
        <title>Scenari</title>
        <icon>images/layer-scene.png</icon>
      </layer>
      <layer id="light">
        <title>Illuminazione</title>
        <icon>images/layer-light.png</icon>
      </layer>
      <layer id="blind">
        <title>Serramenti</title>
        <icon>images/layer-blind.png</icon>
      </layer>
      <layer id="security">
        <title>Sicurezza</title>
        <icon>images/layer-security.png</icon>
      </layer>
      <layer id="video">
        <title>Video</title>
        <icon>images/layer-video.png</icon>
      </layer>
      <layer id="audio">
        <title>Audio</title>
        <icon>images/layer-audio.png</icon>
      </layer>
      <layer id="thermo">
        <title>Termoregolazione</title>
        <icon>images/layer-thermo.png</icon>
      </layer>
      <layer id="power">
        <title>Controllo carichi</title>
        <icon>images/layer-power.png</icon>
      </layer>
    </layers>
    <!-- definizione delle pagine -->
    <pages>  
      <page id="screensaver">
        <title>AUI screensaver - clicca per accedere</title>
        <src>images/ascia_logo_home.png</src>
        <area>
          <title>Piani</title>
          <shape>rect</shape>
          <coords>0,0,320,460</coords>
          <page>piani</page>
        </area>
      </page>      
      <page id="piani">    
        <title>Piani</title>
        <src>images/assonometria320x370.png</src>
        <control id="back">
          <title>Screensaver</title>
          <type>page</type>
          <page>screensaver</page>
          <left>0</left>
          <top>0</top>
        </control>      
        <area>
          <title>Test</title>
          <coords>0,0,320,120</coords>
          <page>test</page>
        </area>
        <area>
          <title>Home in the Box</title>
          <coords>0,121,320,240</coords>
          <page>Hbox</page>
        </area>
        <area>
          <title>Piano Terra</title>
          <coords>0,241,320,370</coords>
          <page>pianoT</page>
        </area>
      </page>
      <page id="test">
        <src>images/piani-all.png</src>
        <control id="back">
          <title>Torna alla selezione dei piani</title>
          <type>page</type>
          <page>piani</page>
          <left>0</left>
          <top>0</top>
        </control>      
      </page>      
      <page id="pianoT">    
        <title>Mappa piano terra</title>
        <src>images/planimetria-big.png</src>
        <control id="back">
          <title>Torna alla selezione dei piani</title>
          <type>page</type>
          <page>piani</page>
          <left>0</left>
          <top>0</top>
        </control>      
        <control id="lc">
          <title>Luce camera</title>
          <type>light</type>
          <layer>light</layer>
          <left>150</left>
          <top>160</top>
          <address>2.3:Out5</address>
        </control>      
        <control id="tc">
          <title>Tapparella camera</title>
          <type>blind</type>
          <layer>blind</layer>
          <left>145</left>
          <top>50</top>
          <address>2.3:Blind1</address>
        </control>      
        <control id="lb">
          <title>Luce bagno</title>
          <type>light</type>
          <layer>light</layer>
          <left>280</left>
          <top>150</top>
          <address>2.3:Out5</address>
        </control>      
        <control id="bb">
          <title>Tapparella bagno</title>
          <type>blind</type>
          <layer>blind</layer>
          <left>295</left>
          <top>50</top>
          <address>2.3:Blind2</address>
        </control>      
        <control id="m1l2">
          <title>Luce scale</title>
          <type>dimmer</type>
          <layer>light</layer>
          <left>100</left>
          <top>300</top>
          <address>2.5:Out1</address>
        </control>      
      </page>
      <page id="Hbox">
        <title>Home in The Box</title>
        <src>images/HBox.jpg</src>
        <control id="back">
           <title>Torna alla selezione dei piani</title>
          <type>page</type>
          <page>piani</page>
           <left>500</left>
           <top>100</top>
        </control>      
        <control id="l1">
           <title>Luce scale</title>
           <type>light</type>
           <layer>light</layer>
           <top>145</top>
           <left>105</left>
           <address>2.3:Out5</address>
        </control>
        <control id="l2">
           <title>Luce ingresso</title>
           <type>dimmer</type>
           <layer>light</layer>
           <top>225</top>
           <left>495</left>
           <address>2.5:Out1</address>
        </control>
        <control id="porta">
           <title>Apriporta</title>
           <type>pushbutton</type>
           <layer>blind</layer>
           <top>295</top>
           <left>495</left>
           <address>2.3:Out8</address>
	         <default>images/door-close.png</default>
	         <on>images/door-open.png</on>
	         <off>images/door-close.png</off>             
        </control>
        <control id="allarme1">
           <title>Allarme</title>
           <type>group</type>
           <layer>security</layer>
           <top>50</top>
           <left>50</left>
           <address>2.Group1</address>
           <value>true</value>
        </control>
        <control id="allarme2">
           <title>Allarme</title>
           <type>group</type>
           <layer>security</layer>
           <top>50</top>
           <left>110</left>
           <address>2.Group1</address>
           <value>false</value>
        </control>
        <control id="b1">
           <title>Tapparella lunga</title>
           <type>blind</type>
           <layer>blind</layer>
           <top>75</top>
           <left>15</left>
           <address>2.3:Blind1</address>
        </control>
        <control id="b2">
           <title>Tapparella corta</title>
           <type>blind</type>
           <layer>blind</layer>
           <top>60</top>
           <left>95</left>
           <address>2.3:Blind2</address>
        </control>
        <control id="c1">
          <title>Cronotermostato HI</title>
          <type>thermo</type>
          <layer>thermo</layer>
          <top>300</top>
          <left>10</left>
          <address>2.7:temp</address>
          <address>2.7:season</address>
          <address>2.7:state</address>
          <up_address>2.7:stp_up</up_address>
          <dn_address>2.7:stp_up</dn_address>
        </control>
        <control id="c2">
          <title>Cronotermostato WDB</title>
          <type>thermo</type>
          <layer>null</layer>
          <top>0</top>
          <left>500</left>
          <address>1.255:temp</address>
        </control>
        <control id="p1">
          <title>Power</title>
          <type>power</type>
          <layer>power</layer>
          <top>120</top>
          <left>570</left>
          <address>2.3:Out7</address>
        </control>
        <control id="cam1">
          <title>Telecamera</title>
          <type>webcam</type>
          <top>150</top>
          <left>150</left>
          <width>320</width>
          <height>240</height>
          <layer>security</layer>
          <video>http://video.ascia.it/axis-cgi/mjpg/video.cgi?resolution=320x240&amp;fps=1</video>
        </control>      
        <control id="reset">
          <title>Reset</title>
          <type>digitalinput</type>
          <layer>power</layer>
          <top>320</top>
          <left>470</left>
          <address>2.3:Inp5</address>
        </control>
           <control id="SceneEntrata">
           <title>Entrata</title>
           <type>scene</type>
           <layer>scene</layer>
           <top>10</top>
           <left>400</left>
           <address>2.6:Scene1</address>
	        <default>images/scene-enter.png</default>
      	</control>
      	<control id="SceneUscita">
           <title>Uscita</title>
           <type>scene</type>
           <layer>scene</layer>
           <top>10</top>
           <left>460</left>
           <address>2.6:Scene2</address>
	        <default>images/scene-exit.png</default>
      	</control>   
      	<control id="SceneNight">
           <title>Notte</title>
           <type>scene</type>
           <layer>scene</layer>
           <top>80</top>
           <left>380</left>
           <address>2.6:Scene3</address>
	        <default>images/scene-night.png</default>
      	</control>
      	<control id="SceneDay">
           <title>Giorno</title>
           <type>scene</type>
           <layer>scene</layer>
           <top>80</top>
           <left>440</left>
           <address>2.6:Scene4</address>
	        <default>images/scene-relax.png</default>
      	</control>    
      </page>
      <page id="chrono_c1">
        <control id="back">
           <title>Indietro</title>
          <type>page</type>
          <page>Hbox</page>
           <top>300</top>
           <left>130</left>
        </control>      
        <control id="c1">
          <title>Cronotermostato HI</title>
          <type>thermo</type>
          <class>ThermoEDS</class>
          <layer>thermo</layer>
          <top>300</top>
          <left>10</left>
          <address>2.7:temp</address>
          <address>2.7:season</address>
          <address>2.7:state</address>
          <address>2.7:setPoint</address>
          <up_address>2.7:stp_up</up_address>
          <dn_address>2.7:stp_up</dn_address>
          <chrono_address>2.7:chrono</chrono_address>
          <manual_address>2.7:manual</manual_address>
          <off_address>2.7:manual</off_address>
          <temp_t0_address>2.7:manual</temp_t0_address>
        </control>     
      </page>
    </pages>
  </AUI>
</jais:configuration>
