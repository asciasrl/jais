<?xml version="1.0" encoding="UTF-8"?>
<jais:configuration version="1.3">
	<sequencer>
		<sequence id="blink1">
		  <step>2.5:Out1=100</step>
		  <step>wait=500</step>
		  <step>2.5:Out1=0</step>
		  <step>wait=500</step>
		  <step>restart</step>
		</sequence>
		<sequence id="blink2">
		  <step>2.3:Out5=on</step>
		  <step>wait=500</step>
		  <step>2.3:Out5=off</step>
		  <step>wait=500</step>
		  <step>restart</step>
		</sequence>
		<sequence id="uscita">
		  <step>2.5:Out1=10</step>
		  <step>2.3:Blind1=close</step>
		  <step>2.3:Blind2=close</step>
		  <step>wait=7000</step>
		  <step>2.5:Out1=0</step>
		  <step>wait=1000</step>
		  <step>2.3:Out5=on</step>
		  <step>2.3:Out8=on</step>
		  <step>wait=100</step>
		  <step>2.3:Out8=off</step>
		  <step>wait=3000</step>
		  <step>2.3:Out5=off</step>
		</sequence>
	</sequencer>
</jais:configuration>
