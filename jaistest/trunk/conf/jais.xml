<?xml version="1.0" encoding="ISO-8859-1" ?>
<!-- commento -->
<jais>
  <modules>
    <module>
      <name>HTTPServer</name>
      <class>it.ascia.ais.HTTPServerControllerModule</class>
    </module>
    <module>
      <name>EDS</name>
      <class>it.ascia.eds.EDSControllerModule</class>
    </module>
    <module>
      <name>DXP</name>
      <class>it.ascia.dxp.DXPControllerModule</class>
    </module>
    <module>
      <name>AUI</name>
      <class>it.ascia.aui.AUIControllerModule</class>
    </module>
  </modules>
  <HTTPServer>
    <address>80</address>
    <root>../aui</root>
    <debug>false</debug>
    <verbose>false</verbose>
    <servlets>
	    <servlet>
	    	<class>it.ascia.aui.AUIStreamingServlet</class>
	    	<context>/stream</context>
	    </servlet>
	    <servlet>
	    	<class>it.ascia.aui.AUICommandServlet</class>
	    	<context>/jais</context>
	    </servlet>
	   </servlets>
  </HTTPServer>
  <EDS>
    <autoupdate>100</autoupdate>
    <connectors>
      <!--
      <connector>
        <name>0</name>
        <transport>
          <type>serial</type>
          <port>COM1</port>
          <speed>9600</speed>
        </transport>
		    <computer>249</computer>
		    <discover>2;3;4;5;6;7</discover> 
      </connector>
			-->
			<!--
      <connector>
        <name>0</name>
        <transport>
          <type>tcp</type>
          <host>copper.ascia.it</host>
          <prot>10001</port>
        </transport>
		    <computer>250</computer>
		    <discover>2;3;4;5;6;7</discover> 
      </connector>
      -->
      <connector>
        <name>1</name>
        <transport>
          <type>serial</type>
          <speed>19200</speed>
          <port>COM1</port>
        </transport>
		    <computer>251</computer>
		    <discover>255</discover> 
      </connector>
      <connector>
        <name>2</name>
        <transport>
          <type>serial</type>
          <speed>9600</speed>
          <port>COM2</port>
        </transport>
		    <computer>252</computer>
		    <discover>2;3;4;5;6;7</discover>
      </connector>
    </connectors> 
  </EDS>
  <DXP>
		<autoupdate>0</autoupdate>
		<connectors> 
			<connector>
			  <name>dxp1</name>
			  <transport>
			    <type>serial</type>
			    <speed>19200</speed>
			    <port>COM2</port>
			  </transport>
				<equ>081231_ValigiaDomino_a_Ang.equ</equ>
				<devices>
					<device>
						<model>DFGSM2</model>
						<address>245</address>
					</device>
					<device>
						<model>DF4IV</model>
						<address>129</address>
					</device>
					<device>
						<model>DFIR</model>
						<address>13</address>
					</device>
					<device>
						<model>DFTA</model>
						<address>18</address>
					</device>
					<device>
						<model>DFCT</model>
						<address>100</address>
					</device>
					<device>
						<model>DF8IL</model>
						<address>9</address>
					</device>
					<device>
						<model>DF4R</model>
						<address>1</address>
					</device>
					<device>
						<model>DF4R</model>
						<address>2</address>
					</device>
					<device>
						<model>DFTP</model>
						<address>3</address>
					</device>
				</devices>				
			</connector>
		</connectors>
  </DXP>      
  <AUI>
    <startPage>Hbox</startPage>
    <title>ASCIA HomeInTheBox [AUI 0.1]</title>
    <skin>skins/20090330/</skin>
    <!-- definizione di tutti i tipi di controlli -->
    <controls>
      <page>
        <default>images/page_back.png</default>
      </page>    
      <light>
        <default>images/light_off.png</default>
        <on>images/light_on.png</on>
        <off>images/light_off.png</off>  
      </light>
      <dimmer>
        <default>images/light_off.png</default>
        <on>images/light_on.png</on>
        <off>images/light_off.png</off>
        <up>images/light_up.png</up>
        <down>images/light_down.png</down>  
        <step>10</step>
        <timer>300</timer>
        <max>85</max>
        <min>0</min>
      </dimmer>
      <blind>
        <default>images/blind_stopped.png</default>
        <stopped>images/blind_stopped.png</stopped>
        <open>images/blind_open.png</open>
        <opening>images/blind_opening.png</opening>
        <opened>images/blind_opened.png</opened>
        <close>images/blind_close.png</close>
        <closing>images/blind_closing.png</closing>
        <closed>images/blind_closed.png</closed>
      </blind>
      <power>
      	<default>images/power_off.png</default>
      	<on>images/power_on.png</on>
      	<off>images/power_off.png</off>
      </power>
      <thermo>
				<default>images/thermo_display.png</default>
				<crono>images/thermo_crono.png</crono>
				<down>images/thermo_down.png</down>
				<fan0>images/thermo_fan0.png</fan0>
				<fan1>images/thermo_fan1.png</fan1>
				<fan2>images/thermo_fan2.png</fan2>
				<fan3>images/thermo_fan3.png</fan3>
				<manual_off>images/thermo_manual_off.png</manual_off>
				<manual_on>images/thermo_manual_on.png</manual_on>
				<switch_on>images/thermo_switch_on.png</switch_on>
				<T1_off>images/thermo_T1_off.png</T1_off>
				<T1_on>images/thermo_T1_on.png</T1_on>
				<T2_off>images/thermo_T2_off.png</T2_off>
				<T2_on>images/thermo_T2_on.png</T2_on>
				<T3_off>images/thermo_T3_off.png</T3_off>
				<T3_on>images/thermo_T3_on.png</T3_on>
				<up>images/thermo_up.png</up>
      </thermo>      
    </controls>
    <!-- definizione di tutti i layers utilizzabili nelle pagine -->
    <layers>
      <scenari>
        <icon>images/scenari.png</icon>
      </scenari>
      <illuminazione>
        <icon>images/illuminazione.png</icon>
      </illuminazione>
      <serramenti>
        <icon>images/serramenti.png</icon>
      </serramenti>
      <sicurezza>
        <icon>images/serramenti.png</icon>
      </sicurezza>
      <video>
        <icon>images/video.png</icon>
      </video>
      <clima>
        <icon>images/clima.png</icon>
      </clima>
      <audio>
        <icon>images/audio.png</icon>
      </audio>
      <energia>
        <icon>images/energia.png</icon>
      </energia>
      <scenari>
        <icon>images/scenari.png</icon>
      </scenari>
    </layers>
    <pages>  
      <page id="screensaver">
        <title>AUI screensaver - clicca per accedere</title>
        <src>images/ascia_logo_home.png</src>
        <area>
          <title>Piani</title>
          <coords>0,0,320,460</coords>
          <page>piani</page>
        </area>
      </page>      
      <page id="piani">    
        <title>Piani</title>
        <src>custom/images/assonometria320x370.png</src>
      	<control id="back">
  	      <title>Screensaver</title>
	        <type>page</type>
	        <page>screensaver</page>
        	<left>0</left>
        	<top>0</top>
	      </control>      
        <area>
          <title>Primo piano</title>
          <coords>0,0,320,120</coords>
          <page>pianoT</page>
        </area>
        <area>
          <title>Piano terra</title>
          <coords>0,121,320,240</coords>
          <page>Hbox</page>
        </area>
        <area>
          <title>Seminterrato</title>
          <coords>0,241,320,370</coords>
          <page>test</page>
        </area>
      </page>
      <page id="test">
        <src>images/piani-all.png</src>
      	<control id="back">
  	      <title>Torna alla selezione dei piani</title>
	        <type>page</type>
	        <page>piani</page>
        	<left>0</left>
        	<top>0</top>
	      </control>      
      </page>      
      <page id="pianoT">    
				<title>Mappa piano terra</title>
				<src>custom/images/planimetria-big.png</src>
      	<control id="back">
  	      <title>Torna alla selezione dei piani</title>
	        <type>page</type>
	        <page>piani</page>
        	<left>0</left>
        	<top>0</top>
	      </control>      
  	    <control id="lc">
  	      <title>Luce camera</title>
  	      <type>light</type>
      	  <layer>illuminazione</layer>
        	<left>150</left>
        	<top>160</top>
	        <address>2.3:Out5</address>
  	    </control>      
    	  <control id="tc">
  	      <title>Tapparella camera</title>
  	      <type>blind</type>
      	  <layer>serramenti</layer>
      	  <left>145</left>
        	<top>50</top>
	        <address>2.3:Out1</address>
  	    </control>      
  	    <control id="lb">
  	      <title>Luce bagno</title>
  	      <type>light</type>
      	  <layer>illuminazione</layer>
        	<left>280</left>
        	<top>150</top>
	        <address>2.3:Out5</address>
  	    </control>      
    	  <control id="bb">
  	      <title>Tapparella bagno</title>
  	      <type>blind</type>
      	  <layer>serramenti</layer>
      	  <left>295</left>
        	<top>50</top>
	        <address>2.3:Out3</address>
  	    </control>      
  	    <control id="m1l2">
  	      <title>Luce scale</title>
  	      <type>dimmer</type>
      	  <layer>illuminazione</layer>
        	<left>100</left>
        	<top>300</top>
	        <address>2.5:Out0</address>
  	    </control>      
      </page>
      <page id="Hbox">
				<title>Home in The Box</title>
				<src>images/HBox.jpg</src>
	    	<control id="back">
	 	      <title>Torna alla selezione dei piani</title>
	        <type>page</type>
	        <page>piani</page>
	       	<left>500</left>
	       	<top>100</top>
	      </control>      
				<control id="l1">
				 	<title>Luce scale</title>
				 	<type>light</type>
				 	<layer>illuminazione</layer>
				 	<top>145</top>
				 	<left>105</left>
				 	<address>2.3:Out5</address>
				</control>
				<control id="l2">
				 	<title>Luce ingresso</title>
				 	<type>dimmer</type>
				 	<layer>illuminazione</layer>
				 	<top>225</top>
				 	<left>495</left>
				 	<address>2.5:Out1</address>
				</control>
				<control id="b1">
				 	<title>Tapparella lunga</title>
				 	<type>blind</type>
				 	<layer>blinds</layer>
				 	<top>45</top>
				 	<left>15</left>
				 	<address>2.3:Blind1</address>
				</control>
				<control id="b2">
				 	<title>Tapparella corta</title>
				 	<type>blind</type>
				 	<layer>blinds</layer>
				 	<top>20</top>
				 	<left>95</left>
				 	<address>2.3:Blind2</address>
				</control>
				<control id="c1">
				 	<title>Cronotermostato HI</title>
				 	<type>thermo</type>
				 	<layer>thermo</layer>
				 	<top>200</top>
				 	<left>300</left>
				 	<address>2.7:temp</address>
				</control>
				<control id="c2">
				 	<title>Cronotermostato WDB</title>
				 	<type>thermo</type>
				 	<layer>thermo</layer>
				 	<top>0</top>
				 	<left>500</left>
				 	<address>1.255:temp</address>
				</control>
				<control id="p1">
				 	<title>Power</title>
				 	<type>power</type>
				 	<layer>power</layer>
				 	<top>120</top>
				 	<left>570</left>
				 	<address>2.3:Out7</address>
				</control>
      </page>
    </pages>
  </AUI>
</jais>
