      <name>HTTPServer</name>
      <class>it.ascia.ais.HTTPServerControllerPlugin</class>
    </plugin>
    <plugin>
      <name>EDS</name>
      <class>it.ascia.eds.EDSControllerPlugin</class>
    </plugin>
  </plugins>
  <HTTPServer>
